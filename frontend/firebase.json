{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|svg|webp)",
        "headers": [ {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        } ]
      },
      {
        "source": "**/*.@(js)",
        "headers": [ {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        } ]
      }
    ],
    "cleanUrls": true
  }
}
